[
  {
    "image": "yukofeb/hubot_in_docker:v_--CIRCLE_BUILD_NUM--",
    "name": "myhubot",
    "cpu": 10,
    "memory": 400,
    "essential": true,
    "entryPoint": [
      "bin/hubot",
      "-a",
      "slack",
      "-d"
    ],
    "environment": [
      { "name" : "HUBOT_SLACK_TOKEN", "value" : "--HUBOT_SLACK_TOKEN--" }
    ],
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ]
  }
]
